    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.6.4",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.8.4"
  }
