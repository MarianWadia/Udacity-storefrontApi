    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "pg": "^8.5.1",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.9.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/pg": "^7.14.7",
