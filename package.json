
{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "build": "npx tsc",
    "prettier": "prettier --ignore-path .prettierignore \"src/**/*.ts\" --write",
    "lint": "eslint --ignore-path .eslintignore --ext .ts .",
    "migration-dev-up": "db-migrate up",
    "migration-dev-reset": "db-migrate reset",
    "migration-testdb-reset": "db-migrate --env test reset",
    "migration-testdb-up": "db-migrate --env test up",
    "migration-test:windows": "set ENV=test&&db-migrate --env test up&& jasmine-ts&&db-migrate --env test reset",
    "migration-test:mac": "export ENV=test&&db-migrate --env test up&& jasmine-ts&&db-migrate --env test reset"
  },
